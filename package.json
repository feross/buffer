    "ieee754": "~1.1.1"
  },
  "devDependencies": {
    "benchmark": "^1.0.0",
    "browserify": "^3.46.0",
    "tape": "^2.12.3"
  },
  "homepage": "http://feross.org",
  "keywords": [
    "buffer",
    "browserify",
    "compatible",
