{
  "name": "native-buffer-browserify",
  "version": "0.5.0",
  "description": "buffer module compatibility for browserify (backed by ArrayBuffer so its fast!)",
  "main": "index.js",
  "dependencies": {
    "base64-js": "0.0.4"
  },
  "devDependencies": {
    "tape": "~2.1.0",
    "benchmark": "~1.0.0",
    "browserify": "~2.35.4"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/feross/native-buffer-browserify.git"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/20..latest",
      "firefox/10..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6"
    ]
  },
  "keywords": [
    "buffer",
    "browserify",
    "compatible",
    "meatless",
    "browser",
    "arraybuffer",
    "uint8array",
    "dataview"
  ],
  "author": "Feross Aboukhadijeh <feross@feross.org> (http://feross.org/)",
  "contributors": [
    "Romain Beauxis <toots@rastageeks.org>"
  ],
  "scripts": {
    "test": "tape test/*.js"
  },
  "license": "MIT",
  "engine": {
    "node": ">=0.6"
  }
}
